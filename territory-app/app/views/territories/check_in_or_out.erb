#Allow logged in user to view all territories checked out to them and check them back in or link to the terriories.erb page to
#check out a new territory.
<h1>Check Out a Territory</h1>
<br></br>
<form action="/check_in_or_out" method="POST"
<input type="checkbox" name="<%=territory.id%>" value="#<%=territory.id%>"> <%=territory.name%><br>
  <input type="checkbox" name="<%=territory.id%>" value="#<%=territory.id%>" checked> <%=territory.name%><br>
  <input type="submit" value="Submit">
</form>
<br></br>
<h1>Return a Territory</h1>
<br></br>
<form action="/check_in_or_out" method="POST"
<input type="checkbox" name="<%=territory.id%>" value="<%=territory.id%>"><%= %> <br>
  <input type="checkbox" name="<%=territory.id%>" value="<%=territory.id%>" checked> <%=territory.name %> <br>
  <input type="checkbox" name="<%=territory.id%>" value="<%=territory.id%>" checked> <%=territory.name %> <br>
  <input type="submit" value="Submit">
</form>
